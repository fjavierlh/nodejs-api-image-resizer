{
  "name": "nodejs-api-image-resizer",
  "version": "1.0.0",
  "description": "An API Rest made with NodeJS and Express to resize images",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "preserve": "npm run build",
    "start:gcf": "cd google-cloud-function && npm run dev",
    "db:up": "docker-compose up -d",
    "serve": "npm run db:up && cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/start.js\"",
    "start": "concurrently --kill-others \"npm run start:gcf\" \"npm run serve\"  ",
    "prestart": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "author": "fjavierlh",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.18.1",
    "express-multipart-file-parser": "^0.1.2",
    "express-promise-router": "^4.1.1",
    "form-data": "^4.0.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.1",
    "helmet": "^5.0.2",
    "http-status": "^1.5.1",
    "image-size": "^1.0.1",
    "multer": "^1.4.4",
    "node-fetch": "^2.6.7",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sequelize": "^6.19.0",
    "typedi": "^0.10.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/crypto-js": "^4.1.1",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.13",
    "@types/form-data": "^2.5.0",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/helmet": "^4.0.0",
    "@types/image-size": "^0.8.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.31",
    "@types/node-fetch": "^2.6.1",
    "@types/sequelize": "^4.28.12",
    "@types/sharp": "^0.30.2",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.14.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  }
}
